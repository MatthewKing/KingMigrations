<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>KingMigrations.Sqlite</PackageId>
    <Title>KingMigrations (SQLite)</Title>
    <Description>SQLite migrator for KingMigrations.</Description>
    <VersionPrefix>1.1.0</VersionPrefix>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\KingMigrations\KingMigrations.csproj" />
  </ItemGroup>

</Project>
